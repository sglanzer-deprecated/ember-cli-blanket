{
  "name": "ember-cli-blanket",
  "version": "0.9.9",
  "description": "Blanket code coverage for ember-cli",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node tests/runner",
    "autotest": "mocha --watch --reporter spec tests/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sglanzer/ember-cli-blanket.git"
  },
  "keywords": [
    "ember",
    "ember-cli",
    "ember-addon",
    "code coverage"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Steven Glanzer <sglanzer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sglanzer/ember-cli-blanket/issues"
  },
  "homepage": "https://github.com/sglanzer/ember-cli-blanket",
  "devDependencies": {
    "chai": "^2.2.0",
    "ember-cli": "1.13.8",
    "glob": "^5.0.3",
    "mocha": "^2.2.1",
    "rewire": "^2.5.1",
    "tmp-sync": "^1.0.1"
  },
  "dependencies": {
    "body-parser": "^1.12.2",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "core-object": "^1.1.0",
    "debug": "^2.2.0",
    "ember-cli-version-checker": "1.1.6",
    "fs-extra": "^0.18.0",
    "lodash": "^3.6.0"
  },
  "ember-addon": {
    "after": "ember-cli-qunit",
    "before": "proxy-server-middleware",
    "configPath": "tests/dummy/config"
  }
}
